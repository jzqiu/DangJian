@model System.Collections.Generic.IEnumerable<DangJian.ViewModels.QuotaRecordVM>

<table class="table">
    @foreach (var item in Model)
    {
        <tr>
            <td style="width:700px;">@Html.DisplayFor(modelItem => item.Description)</td>
            <td style="width:60px;">
                @if(item.IsNeed)
                {

                    @Html.ActionLink(item.FillInfo, "Fill", new { QuotaCode = item.QuotaCode });
                }else{
                    <span>无需填报</span>
                }
            </td>
        </tr>
    }
</table>